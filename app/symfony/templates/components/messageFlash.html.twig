<div class="row">
    {% for flashMessage in app.session.flashBag.get('success') %}
        <div class="card-panel green white-text">
            {{ flashMessage }}
        </div>
    {% endfor %}
    {% for flashMessage in app.session.flashBag.get('error') %}
        <div class="card-panel red white-text">
            {{ flashMessage }}
        </div>
    {% endfor %}
</div>